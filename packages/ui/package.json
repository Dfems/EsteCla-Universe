{
  "name": "@estecla/ui",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc -b",
    "type-check": "tsc --noEmit",
    "lint": "echo no-lint",
    "format": "prettier --write . --ignore-path ../../.prettierignore"
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./feedback": {
      "types": "./src/feedback/index.ts",
      "import": "./src/feedback/index.ts"
    },
    "./auth": {
      "types": "./src/auth/index.ts",
      "import": "./src/auth/index.ts"
    },
    "./social": {
      "types": "./src/social/index.ts",
      "import": "./src/social/index.ts"
    }
  },
  "sideEffects": false,
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18",
    "@chakra-ui/react": ">=2"
  }
}
